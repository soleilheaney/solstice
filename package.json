    "@tanstack/react-router-with-query": "alpha",
    "@tanstack/react-start": "alpha",
    "better-auth": "^1.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "lucide-react": "^0.514.0",
